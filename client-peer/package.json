{
  "name": "snapshot-demo-client",
  "version": "0.1.0",
  "description": "Demo for JS PoC peer node",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/*"
  ],
  "bic": [
    "client-peer/*",
    "*.aqua",
    "package-lock.json"
  ],
  "dependencies": {
    "@fluencelabs/aqua-lib": "^0.1.14",
    "@fluencelabs/fluence": "^0.14.2",
    "@fluencelabs/fluence-network-environment": "^1.0.10",
    "it-all": "^1.0.5"
  },
  "scripts": {
    "compile-aqua": "aqua -i ../aqua-scripts -o src/_aqua/",
    "prebuild": "npm run compile-aqua",
    "build": "tsc",
    "prestart:local": "npm run build",
    "start:local": "node dist/src/index.js local",
    "prestart:remote": "npm run build",
    "start:remote": "node dist/src/index.js krasnodar",
    "start": "npm run start:remote"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluencelabs/eip712-validation-node/tree/main/client-peer"
  },
  "keywords": [
    "aqua",
    "fluence"
  ],
  "author": "Fluence Labs",
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/fluencelabs/eip712-validation-node/issues"
  },
  "homepage": "git+https://github.com/fluencelabs/eip712-validation-node/blob/main/README.md",
  "devDependencies": {
    "@fluencelabs/aqua": "^0.4.0-235",
    "typescript": "^4.0.0"
  }
}
